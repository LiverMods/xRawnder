    ValkSection:NewButton("Arrest All", "Cuidado con otros Scripters!", function()
wait(0.1)
local player = game.Players.LocalPlayer
local playerRootPart = player.Character.HumanoidRootPart
local playerCFrame = playerRootPart.CFrame

for _, criminal in pairs(game.Teams.Criminals:GetPlayers()) do
    if criminal.Name ~= player.Name and criminal.Name ~= "jordan_NSS4" then
        local countdown = 15
        repeat
            wait()
            countdown = countdown - 1
            game.Workspace.Remote.arrest:InvokeServer(criminal.Character.HumanoidRootPart)
            playerRootPart.CFrame = criminal.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1)
        until countdown == 0
    end
end
end)
 
 
 
