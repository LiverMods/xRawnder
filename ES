           OtherSection:NewButton("Spider Man", "caminar en las paredes!", function()
        loadstring(game:HttpGet("https://pastebin.com/raw/zXk4Rq2r"))()
    end)    
    OtherSection:NewKeybind("Press T to get TpTool", "usar para teletransportarse", Enum.KeyCode.T, function()
	mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "TpTool"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)  
    local Tel = Window:NewTab("teletransportarse")
    local TelSection = Tel:NewSection("Prison Life")
    TelSection:NewButton("Base", "Uma viagem rápida", function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-943, 95, 2055)
end)
