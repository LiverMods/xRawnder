end)
local GunSection = Gun:NewSection("Weapon Modifications")


GunSection:NewDropdown("Mod Weapon { trying to be discreet }", "Makes the gun fire more", {"M9", "Remington 870", "AK-47", "Taser"}, function(v)
				local module = nil
				if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
					module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
				elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
					module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
				end
				if module ~= nil then	
					module["StoredAmmo"] = 300
					module["FireRate"] = -math.huge
					module["Spread"] = -math.huge
					module["Range"] = 4000
					module["Bullets"] = 3
					module["ReloadTime"] = -math.huge
					module["AutoFire"] = true
				end
			end)  

GunSection:NewDropdown("Weapon Mod", "Makes the weapon fire more shots", {"M9", "Remington 870", "AK-47", "Taser"}, function(v)
				local module = nil
				if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
					module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
				elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
					module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
				end
				if module ~= nil then
					module["MaxAmmo"] = math.huge
					module["StoredAmmo"] = math.huge
					module["FireRate"] = -math.huge
					module["Spread"] = -math.huge
                    module["AmmoPerClip"] = math.huge
                    module["CurrentAmmo"] = math.huge
					module["Range"] = math.huge
					module["Bullets"] = 7
					module["ReloadTime"] = -math.huge
					module["AutoFire"] = true
				end
			end)  

