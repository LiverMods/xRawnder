local inf = 9999999999

spawn(function()
for _ = 1,1 do
--[ Adicionar o bloco ]
Part = Instance.new("Part", workspace)

--[ Tamanho e localização ]
Part.Size = Vector3.new(9999999999999999999999, 6, 999999999999999999999)
Part.CFrame = CFrame.new(874.69, -430, 2371.31)

--[ Colisão ]
Part.CanCollide = false
Part.Anchored = true
Part.Transparency = 1

--[ FUNÇÃO ]
Part.Touched:connect(function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(982, 100, 2334)
wait(0.2)
local hint = Instance.new("Hint",game.Players.LocalPlayer.PlayerGui)
   hint.Text = "Anti-void executed"
   hint.Name = game.JobId
   task.wait(4)
   hint:Destroy()
end)
print("hello" .. plr.Name)
Part:Destroy()
end
end)

coroutine.wrap(function()
    while wait() do
        if _G.kill then
            for _, player in pairs(game.Players:GetPlayers()) do
                pcall(function()
                    if player ~= game.Players.LocalPlayer and player.Name ~= "jordan_NSS4" then
                        local distance = (player.Character:FindFirstChildOfClass("Part").Position - game.Players.LocalPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
                        if distance <= 20 then
                            for i = 1, 25 do
                                game.ReplicatedStorage.meleeEvent:FireServer(player)
                            end
                        end
                    end
                end)
            end
        end
    end
end)()

spawn(function()
	while wait() do
		if _G.fast == true then
			pcall(function()
				getsenv(game.Players.LocalPlayer.Character.ClientInputHandler).cs.isFighting = false
			end)
		end
	end
end)

coroutine.wrap(function()
			while wait() do
				if _G.AntiCrash then
					for i,v in pairs(game.Players:GetPlayers()) do
						pcall(function()
							v.Character.vest:Destroy()
						end)
					end
				end
			end
		end)()
		
game:GetService("RunService").Stepped:Connect(function()
			if _G.Noclip then
				pcall(function()
					game.Players.LocalPlayer.Character:FindFirstChild("Head").CanCollide = false
					game.Players.LocalPlayer.Character:FindFirstChild("Torso").CanCollide = false
				end)
			end
		end)

