local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Maikderninja/Maikderninja/main/ui%20lib%20fix.lua"))();

local Window = Library.CreateLib("x-Rawnder HITBOX UNIVERSAL + Prison Life V2", "BloodTheme", blue)
    local Main = Window:NewTab("Hitbox")
    local MainSection = Main:NewSection("HITBOX")
    MainSection:NewToggle("HITBOX", "Acerte inimigos, HIT BOX", function(state)
    if state then
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/LiverMods/funct/main/FFFF552'),true))()
    else
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/LiverMods/funct/main/FFFF553'),true))()
    end
end)
MainSection:NewButton("HitBox ball V2 ", "não pode ser desativado", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/LiverMods/funct/main/HitboxV2'))()
end)
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")
    PlayerSection:NewSlider("Walkspeed ", "SPEED!!", 500, 16, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)
    PlayerSection:NewSlider("Jumppower", "JUMP HIGH!!", 350, 50, function(s)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)
    PlayerSection:NewButton("Reset WS/JP", "Resets to all defaults", function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end)
    PlayerSection:NewToggle("Shift Op", "melhor que shift! em alguns jogos", function(state)
        if state then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 24
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 58
        else
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 17
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        end
    end)  
  PlayerSection:NewButton("Voltar para o Respawn", "melhor que reset", function()
local A_1 = "\66\114\111\121\111\117\98\97\100\100"
local Event = game:GetService("Workspace").Remote.loadchar
Event:InvokeServer(A_1)
end)
    PlayerSection:NewButton("ESP PLAYERS", "Veja pessoas atraves das paredes, não podera ser desativado!", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/VapingCat/Open-Source-Chasms-Script/main/script.lua'))()
    end)
            local Gun = Window:NewTab("PRISON Gun")
			local GunSection = Gun:NewSection("Gun")
			
			GunSection:NewDropdown("Pegar GUN", "Dá armas ao jogador", {"M9", "Remington 870", "AK-47"}, function(v)
				local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver[v].ITEMPICKUP
				local Event = game:GetService("Workspace").Remote.ItemHandler
				Event:InvokeServer(A_1)
			end)
			GunSection:NewDropdown("Gun Mod", "Faz a arma dar mas disparos", {"M9", "Remington 870", "AK-47", "Taser"}, function(v)
				local module = nil
				if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
					module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
				elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
					module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
				end
				if module ~= nil then
					module["MaxAmmo"] = math.huge
					module["CurrentAmmo"] = math.huge
					module["StoredAmmo"] = math.huge
					module["FireRate"] = 0.00001
					module["Spread"] = 0
					module["Range"] = math.huge
					module["Bullets"] = 7
					module["ReloadTime"] = 0.00001
					module["AutoFire"] = true
				end
			end)  
    local Other = Window:NewTab("Outros")
