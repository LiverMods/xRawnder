local speedDesertBindable = Instance.new('BindableFunction')
function speedDesertBindable.OnInvoke(state)
	if state == 'join Prison Life' then
		teleportService:Teleport(155615604, player )

	end
end
	game.StarterGui:SetCore('SendNotification', {
		Title = "Error";
	        Text = "no support for this game";
		Duration = math.huge,
		Callback = speedDesertBindable,
		Button1 = 'join Prison Life',
		Button2 = '❌'
	})

end
