-- Cria uma GUI que mostrarÃ¡ a quantidade de jogadores no servidor
local playersGui = Instance.new("ScreenGui")
playersGui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
playersGui.ResetOnSpawn = false

-- Cria um rÃ³tulo que mostrarÃ¡ o nÃºmero de jogadores
local playersLabel = Instance.new("TextLabel")
playersLabel.Position = UDim2.new(1, -400, 0, 10)
playersLabel.Size = UDim2.new(0, 100, 0, 30)
playersLabel.TextScaled = true
playersLabel.Parent = playersGui
playersLabel.BackgroundTransparency = 0.45 -- Define a transparÃªncia do rÃ³tulo
playersLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Define a cor de fundo do rÃ³tulo

-- FunÃ§Ã£o para atualizar a GUI com o nÃºmero atual de jogadores
local function updatePlayersCount()
    local playersCount = #game:GetService("Players"):GetPlayers()
    playersLabel.Text = "Jogadores: " .. playersCount
    
    -- Altera a cor do texto com base no nÃºmero de jogadores
    if playersCount >= 0 and playersCount <= 14 then
        playersLabel.TextColor3 = Color3.new(0, 1, 0) -- Verde
    elseif playersCount >= 15 and playersCount <= 21 then
        playersLabel.TextColor3 = Color3.new(1, 0.6, 0) -- Laranja
    elseif playersCount >= 22 and playersCount <= 24 then
        playersLabel.TextColor3 = Color3.new(1, 0, 0) -- Vermelho
    end
    
    -- Altera a transparÃªncia do rÃ³tulo (nÃ£o precisa ser alterada)
end

-- Chama a funÃ§Ã£o de atualizaÃ§Ã£o inicialmente
updatePlayersCount()

-- Conecta a funÃ§Ã£o de atualizaÃ§Ã£o ao evento de mudanÃ§a de jogadores
game:GetService("Players").PlayerAdded:Connect(updatePlayersCount)
game:GetService("Players").PlayerRemoving:Connect(updatePlayersCount)

end) task.wait(0.05)

